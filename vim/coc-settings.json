{
  "coc.preferences.jumpCommand": "split",
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact"],
	"go.goplsOptions": {
		"buildFlags": ["-gcflags=-G=3"]
	},
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": [
        "serve"
      ],
      "filetypes": ["terraform"],
      "initializationOptions": {}
    }
  }
}
